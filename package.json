{
  "name": "mapit-motorbikes",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173 --host",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "cypress:e2e": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --e2e'",
    "cypress:e2e:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --e2e'",
    "cypress:component": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress open --component'",
    "cypress:component:ci": "start-server-and-test preview http://127.0.0.1:4173/ 'cypress run --component'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .eslintignore"
  },
  "dependencies": {
    "@headlessui/vue": "^1.6.5",
    "google-maps": "^4.3.3",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.41",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "cypress": "^10.1.0",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.4",
    "typescript": "~4.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.1",
    "vue-tsc": "^0.38.1"
  }
}
